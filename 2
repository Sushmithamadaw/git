CREATE TABLE TEAMS_(TID VARCHAR(20)PRIMARY KEY,TNAME VARCHAR(25)NOT NULL,COACH VARCHAR(25),CAPTAIN_ID VARCHAR(20),CITY VARCHAR(20),FOREIGN KEY(CAPTAIN_ID)REFERENCES PLAYERS(PID));
